<div id="product-card" class="card-container d-flex flex-wrap justify-content-center">
  {{#each products}}
  <div class="card" style="width: 18rem;">
    <img src="{{this.photo}}" class="card-img-top" alt="{{this.title}}" />
    <div class="card-body">
      <h5 class="card-title fw-bolder">{{this.title}}</h5>
      <p class="card-text">${{this.price}}</p>
      <p class="card-text fw-light">{{this.stock}} units in stock</p>
    </div>
  </div>
{{/each}}
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/real.js"></script>