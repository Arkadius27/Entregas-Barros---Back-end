<form class="w-50 m-auto my-5">
  <div class="mb-3">
    <label for="InputName" class="form-label">Name</label>
    <input type="text" class="form-control" id="InputName">
  </div>
  <div class="mb-3">
    <label for="inputEmail" class="form-label">Email address</label>
    <input type="email" class="form-control" id="inputEmail" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">Your email isn't shared with anyone.</div>
  </div>
  <div class="mb-3">
    <label for="inputPassword" class="form-label">Password</label>
    <input type="password" class="form-control" id="inputPassword">
    <div id="passwordHelp" class="form-text">Please select a strong password.</div>
  </div>
  <div class="mb-3">
    <label for="profilePic" class="form-label">Profile Picture</label>
    <input type="file" accept=".png, .PNG, .jpg, .JPEG, .WebP" class="form-control" id="profilePic">
  </div>
  <button type="submit" class="btn btn-primary">Register</button>
</form>
<script src="/register.js"></script>